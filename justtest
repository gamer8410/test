-- MAIN TAB - Farming
MainTab:AddSection("Weapon Selection")

MainTab:AddDropdown({
    Name = "Select Weapon",
    Options = {"Melee", "Sword", "Blox Fruits"},
    Default = "Melee",
    Flag = "weapon_select",
    Callback = function(Value)
        ChooseWeapon = Value
    end
})

MainTab:AddSection("Auto Farm")

MainTab:AddToggle({
    Name = "Auto Farm Level",
    Default = false,
    Flag = "auto_level",
    Callback = function(Value)
        AutoLevel = Value
        if Value == false then
            wait()
            -- Stop farming logic
        end
    end
})

MainTab:AddSlider({
    Name = "Farm Distance (Y Axis)",
    Min = 0,
