-- FRUIT TAB
FruitTab:AddSection("Fruit Management")

local FruitList = {"Dragon-Dragon", "Leopard-Leopard", "Spirit-Spirit", "Control-Control", 
                   "Venom-Venom", "Shadow-Shadow", "Dough-Dough", "Gravity-Gravity", "Pain-Pain",
                   "Rumble-Rumble", "Buddha-Buddha", "Quake-Quake", "Light-Light", "Dark-Dark",
                   "Magma-Magma", "Ice-Ice", "Flame-Flame", "Sand-Sand"}

FruitTab:AddDropdown({
    Name = "Select Fruit",
    Options = FruitList,
    Default = "Dragon-Dragon",
    Flag = "fruit_select",
    Callback = function(Value)
        _G.SelectFruit = Value
    end
})

FruitTab:AddToggle({
    Name = "Buy Fruit",
    Default = false,
    Callback = function(Value)
        if Value then
            _G.AutoBuyFruitSniper = true
            pcall(function()
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("GetFruits")
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("PurchaseRawFruit", _G.SelectFruit, false)
            end)
            _G.AutoBuyFruitSniper = false
        end
    end
})

FruitTab:AddDropdown({
    Name = "Select Permanent Fruit",
    Options = FruitList,
    Default = "Dragon-Dragon",
    Flag = "permanent_fruit",
    Callback = function(Value)
        _G.PermanentFruit = Value
    end
})

FruitTab:AddToggle({
    Name = "Use Permanent Fruit",
    Default = false,
    Callback = function(Value)
        if Value then
            _G.AutoSwitchPermanentFruit = true
            pcall(function()
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SwitchFruit", _G.PermanentFruit)
            end)
            _G.AutoSwitchPermanentFruit = false
        end
    end
})

