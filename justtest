    KeyCode = Enum.KeyCode.End
})

-- Mobile Button (optional)
local MobileButton = Minimizer:CreateMobileMinimizer({
    Image = "rbxassetid://130947856929902",
    BackgroundColor3 = Color3.fromRGB(255, 255, 255)
})

