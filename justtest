local function GetChar()
    return LP.Character
end

local function GetHRP()
    local char = GetChar()
    return char and char:FindFirstChild("HumanoidRootPart")
end

--// TWEEN SYSTEM
local TweenService = game:GetService("TweenService")
local TweenSpeed = 300
local currentTween = nil

local function Tween(targetCFrame)
    local hrp = GetHRP()
    if not hrp then return end
    
    -- Cancel existing tween
    if currentTween then
        currentTween:Cancel()
    end
    
    local distance = (targetCFrame.Position - hrp.Position).Magnitude
    local duration = distance / TweenSpeed
    
    local tweenInfo = TweenInfo.new(
        duration,
        Enum.EasingStyle.Linear,
        Enum.EasingDirection.InOut
    )
    
    currentTween = TweenService:Create(hrp, tweenInfo, {CFrame = targetCFrame})
    currentTween:Play()
    
    return currentTween
end

local function StopTween()
    if currentTween then
        currentTween:Cancel()
        currentTween = nil
    end
end

local function EquipTool(toolName)
    local tool = LP.Backpack:FindFirstChild(toolName) or LP.Character:FindFirstChild(toolName)
    if tool and tool:IsA("Tool") then
        LP.Character.Humanoid:EquipTool(tool)
        return true
    end
    return false
end

local function HasKey()
    return LP.Backpack:FindFirstChild("Key") or LP.Character:FindFirstChild("Key")
end

local function EquipWeapon()
    local char = GetChar()
    if not char then return nil end
    
    for _, tool in pairs(char:GetChildren()) do
        if tool:IsA("Tool") and tool.ToolTip ~= ChooseWeapon then
            tool.Parent = LP.Backpack
        end
    end
    
    for _, tool in pairs(char:GetChildren()) do
        if tool:IsA("Tool") and tool.ToolTip == ChooseWeapon then
            return tool
        end
    end
    
    for _, tool in pairs(LP.Backpack:GetChildren()) do
        if tool:IsA("Tool") and tool.ToolTip == ChooseWeapon then
            tool.Parent = char
            return tool
        end
    end
    
    for _, tool in pairs(LP.Backpack:GetChildren()) do
        if tool:IsA("Tool") then
            tool.Parent = char
            return tool
        end
    end
    
    return nil
end

local function GetNearestEnemy()
    if not workspace:FindFirstChild("Enemies") then return nil end
    
    local hrp = GetHRP()
    if not hrp then return nil end
    
    local nearest, minDist = nil, math.huge
    
    for _, enemy in pairs(workspace.Enemies:GetChildren()) do
        local enemyHRP = enemy:FindFirstChild("HumanoidRootPart")
        local enemyHum = enemy:FindFirstChild("Humanoid")
        
        if enemyHRP and enemyHum and enemyHum.Health > 0 then
            local dist = (enemyHRP.Position - hrp.Position).Magnitude
            if dist <= FarmRange and dist < minDist then
                minDist = dist
                nearest = enemy
            end
        end
    end
    
    return nearest
end

local function ExpandHitbox(enemy)
    if not enemy then return end
    local hrp = enemy:FindFirstChild("HumanoidRootPart")
    if hrp then
        hrp.Size = Vector3.new(20, 20, 20)
        hrp.Transparency = 1
        hrp.CanCollide = false
    end
end

local function AutoHaki()
    if not game:GetService("Players").LocalPlayer.Character:FindFirstChild("HasBuso") then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
    end
end


local attackCooldown = 0
local function Attack(enemy)
    if not enemy or tick() - attackCooldown < 0.1 then return end
    attackCooldown = tick()
    
    ExpandHitbox(enemy)
    
    pcall(function()
        local net = ReplicatedStorage:FindFirstChild("Modules")
        if not net then return end
        net = net:FindFirstChild("Net")
        if not net then return end
        
        local regAttack = net:FindFirstChild("RE/RegisterAttack")
        local regHit = net:FindFirstChild("RE/RegisterHit")
        
        if regAttack then
            regAttack:FireServer(0.4)
        end
        
        if regHit then
            local bodyParts = {"HumanoidRootPart", "Torso", "Head", "RightUpperArm", "LeftUpperArm"}
            for _, partName in pairs(bodyParts) do
                local part = enemy:FindFirstChild(partName)
                if part then
                    regHit:FireServer(part, {}, tostring(math.random(100000, 999999)))
                    break
                end
            end
        end
    end)
end

local function EnableFly()
    local hrp = GetHRP()
    if not hrp then return end
    
    if not BV then
        BV = Instance.new("BodyVelocity")
        BV.MaxForce = Vector3.new(1e9, 1e9, 1e9)
        BV.Velocity = Vector3.zero
        BV.Parent = hrp
    end
    
    if not BG then
        BG = Instance.new("BodyGyro")
        BG.MaxTorque = Vector3.new(1e9, 1e9, 1e9)
        BG.P = 9e4
        BG.Parent = hrp
    end
end

local function DisableFly()
    if BV then BV:Destroy(); BV = nil end
    if BG then BG:Destroy(); BG = nil end
end

local function GetQuestStatus()
    local questData = LP.PlayerGui:FindFirstChild("Main")
    if questData then
        questData = questData:FindFirstChild("Quest")
        if questData and questData.Visible then
            return true
        end
    end
    return false
end

local function HasQuest()
    local mainGui = LP.PlayerGui:FindFirstChild("Main")
    if mainGui then
        local questFrame = mainGui:FindFirstChild("Quest")
        if questFrame and questFrame.Visible then
            return true
        end
    end
    return false
end

local function AcceptQuest()
    if HasQuest() then
        return  -- Quest already active, skip
    end
    
    CheckLevel()
    local args = {
        "StartQuest",
        NameQuest,
        QuestLv
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("CommF_"):InvokeServer(unpack(args))
end

local function UpdateNPCSpawnLocations()
    npcSpawnLocations = {}
    
    if not workspace:FindFirstChild("Enemies") then return end
    
    for _, enemy in pairs(workspace.Enemies:GetChildren()) do
        if enemy.Name == NameMon then
            local enemyHRP = enemy:FindFirstChild("HumanoidRootPart")
            if enemyHRP then
                -- Store unique spawn positions
                local pos = enemyHRP.Position
                local isDuplicate = false
                
                for _, storedPos in pairs(npcSpawnLocations) do
                    if (storedPos.Position - pos).Magnitude < 50 then  -- Within 50 studs = same spawn
                        isDuplicate = true
                        break
                    end
                end
                
                if not isDuplicate then
                    table.insert(npcSpawnLocations, enemyHRP.CFrame)
                end
            end
        end
    end
    
    -- If no NPCs found, use default spawn location
    if #npcSpawnLocations == 0 then
        table.insert(npcSpawnLocations, CFrameMon)
    end
end

local function PatrolSpawnArea()
    local hrp = GetHRP()
    if not hrp then return end
    
    -- Update spawn locations every 10 seconds
    if tick() - lastPatrolTime > 10 then
        UpdateNPCSpawnLocations()
        lastPatrolTime = tick()
    end
    
    -- If no spawn locations stored, use default
    if #npcSpawnLocations == 0 then
        Tween(CFrameMon)
        return
    end
    
    -- Get current patrol point
    local currentPatrolPoint = npcSpawnLocations[patrolIndex]
    local distanceToPatrol = (hrp.Position - currentPatrolPoint.Position).Magnitude
    
    -- If reached patrol point, go to next
    if distanceToPatrol < 15 then
        patrolIndex = patrolIndex + 1
        if patrolIndex > #npcSpawnLocations then
            patrolIndex = 1
        end
    end
    
    -- Tween to current patrol point
    Tween(npcSpawnLocations[patrolIndex] * CFrame.new(0, 5, 0))
end

local function GetLevelEnemy()
    if not workspace:FindFirstChild("Enemies") then return nil end
    
    for _, enemy in pairs(workspace.Enemies:GetChildren()) do
        if enemy.Name == NameMon then
            local enemyHum = enemy:FindFirstChild("Humanoid")
            if enemyHum and enemyHum.Health > 0 then
                return enemy
            end
        end
    end
    return nil
end

local function LockEnemy(enemy)
    if not enemy or lockedEnemies[enemy] then return end
    local enemyHRP = enemy:FindFirstChild("HumanoidRootPart")
    local enemyHum = enemy:FindFirstChild("Humanoid")
    
    if enemyHRP and enemyHum then
        enemyHRP.Anchored = true
        enemyHRP.CanCollide = false
        enemyHum.WalkSpeed = 0
        enemyHum.JumpPower = 0
        
        if enemyHum:FindFirstChild("Animator") then
            enemyHum.Animator:Destroy()
        end
        
        lockedEnemies[enemy] = true
    end
end


-- NEW: Unlock enemy when done
local function UnlockEnemy(enemy)
    if not enemy or not lockedEnemies[enemy] then return end
    local enemyHRP = enemy:FindFirstChild("HumanoidRootPart")
    
    if enemyHRP then
        enemyHRP.Anchored = false
    end
    
    lockedEnemies[enemy] = nil
end

task.spawn(function()
    while task.wait() do
        pcall(function()
            if AutoSea2 then
                local playerLevel = game:GetService("Players").LocalPlayer.Data.Level.Value
                
                if playerLevel >= 700 and Sea1 then
                    
                    -- STEP 1: Get key from Detective/Military
                    if not HasKey() then
                        -- Talk to Detective to get key
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("DressrosaQuestProgress", "Detective")
                        task.wait(2)  -- Wait for key to be given
                    end
                    
                    -- STEP 2: Equip key and go to door
                    if HasKey() then
                        EquipTool("Key")
                        task.wait(0.5)
                        
                        -- Tween to the door location
                        local doorPos = CFrame.new(1347.7124, 37.3751602, -1325.6488)
                        repeat
                            Tween(doorPos)
                            EquipTool("Key")  -- Keep equipping key
                            task.wait()
                        until (doorPos.Position - GetHRP().Position).Magnitude <= 5 or not AutoSea2
                        
                        task.wait(1)  -- Wait for door to open
                    end
                    
                    -- STEP 3: Check if door is open, then kill Ice Admiral
                    if game:GetService("Workspace").Map.Ice.Door.Transparency == 1 and 
                       game:GetService("Workspace").Map.Ice.Door.CanCollide == false then
                        
                        -- Door is open, now find and kill Ice Admiral
                        if game:GetService("Workspace").Enemies:FindFirstChild("Ice Admiral") then
                            for _, enemy in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                                if enemy.Name == "Ice Admiral" then
                                    local enemyHRP = enemy:FindFirstChild("HumanoidRootPart")
                                    local enemyHum = enemy:FindFirstChild("Humanoid")
                                    
                                    if enemyHRP and enemyHum and enemyHum.Health > 0 then
                                        -- Lock Ice Admiral in place
                                        LockEnemy(enemy)
                                        
                                        repeat
                                            task.wait()
                                            AutoHaki()
                                            EquipWeapon()
                                            
                                            local hrp = GetHRP()
                                            if hrp then
                                                -- Teleport to Ice Admiral
                                                hrp.CFrame = enemyHRP.CFrame * CFrame.new(0, FarmDistance, 0)
                                            end
                                            
                                            Attack(enemy)
                                            ExpandHitbox(enemy)
                                            
                                        until not AutoSea2 or not enemy.Parent or enemyHum.Health <= 0
                                        
                                        UnlockEnemy(enemy)
                                        task.wait(2)  -- Wait after killing
                                    end
                                end
                            end
                        else
                            -- STEP 4: Ice Admiral is dead, talk to Captain to travel to Sea 2
                            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")
                            task.wait(5)  -- Wait for teleport
                            AutoSea2 = false  -- Turn off after successful travel
                        end
                        
                    end
                    
                end
            end
        end)
    end
end)

-- MAIN LOOP - AUTO FARM LEVEL ONLY
task.spawn(function()
    while task.wait() do
        pcall(function()
            if AutoLevel then
                CheckLevel()
                AcceptQuest()
                
                local enemy = GetLevelEnemy()
                
                if enemy then
                    local enemyHRP = enemy:FindFirstChild("HumanoidRootPart")
                    local enemyHum = enemy:FindFirstChild("Humanoid")
                    
                    if enemyHRP and enemyHum and enemyHum.Health > 0 then
                        -- LOCK ENEMY
                        LockEnemy(enemy)
                        
                        local hrp = GetHRP()
                        if not hrp then return end
                        
                        -- If new enemy, tween once
                        if currentTarget ~= enemy then
                            currentTarget = enemy
                            local distance = (enemyHRP.Position - hrp.Position).Magnitude
                            
                            if distance > 250 then
                                Tween(enemyHRP.CFrame * CFrame.new(0, FarmDistance, 0))
                            else
                                StopTween()
                                hrp.CFrame = enemyHRP.CFrame * CFrame.new(0, FarmDistance, 0)
                            end
                        else
                            -- Stay locked at position (NO JITTER!)
                            StopTween()
                            hrp.CFrame = enemyHRP.CFrame * CFrame.new(0, FarmDistance, 0)
                        end
                        
                        -- Attack
                        EquipWeapon()
                        AutoHaki()
                        Attack(enemy)
                        ExpandHitbox(enemy)
                        
                    else
                        if currentTarget then
                            UnlockEnemy(currentTarget)
                            currentTarget = nil
                        end
                    end
                else
                    -- No enemy - unlock and patrol ACTUAL NPC SPAWN LOCATIONS
                    if currentTarget then
                        UnlockEnemy(currentTarget)
                        currentTarget = nil
                    end
                    
                    PatrolSpawnArea()
                end
            else
                -- Auto farm off - cleanup
                if currentTarget then
                    UnlockEnemy(currentTarget)
                    currentTarget = nil
                end
            end
        end)
    end
end)
-- Final Notification
Window:Notify({
    Title = "Banana Cat Hub",
    Content = "Script loaded successfully!\nConverted to Redz UI by HNC",
    Duration = 5,
    Image = "rbxassetid://10734953451"
})
